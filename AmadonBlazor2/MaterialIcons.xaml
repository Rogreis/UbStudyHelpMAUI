<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AmadonBlazor2.MaterialIcons"
             xmlns:material="clr-namespace:MaterialDesignThemes.XamarinForms;assembly=MaterialDesignThemes.XamarinForms"
             Title="MaterialIcons">
    <VerticalStackLayout>
        <Label 
            Text="Welcome to .NET MAUI!"
            VerticalOptions="Center" 
            HorizontalOptions="Center" />

        <Button Text="Save">
            <material:Icon FontFamily="MaterialDesign" 
                   FontSize="24" 
                   Text="ContentSave" 
                   Margin="0,0,5,0" />
        </Button>


    </VerticalStackLayout>
</ContentPage>